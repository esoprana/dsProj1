<?xml version="1.0" encoding="UTF-8" ?>
<parameters>
	<!--  Simulator parameters -->
	<parameter
		name="fake1"
		displayName="0*********************************************"
		type="java.lang.String"
		defaultValue="******* SIMULATOR PARAMETERS *******"
		isReadOnly="true"
		converter="repast.simphony.parameter.StringConverterFactory$StringStringConverter"/>
	
	<parameter
		name="OUTPUT_FOLDER"
		displayName="00 - Output folder for data OUTPUT_FOLDER"
		type="java.lang.String"
		defaultValue="./output"
		isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$StringStringConverter"/>
	<parameter
		name="STOP_TIME"
		displayName="00 - When to stop automatically (0 for never)"
		type="double"
		defaultValue="0"
		isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"/>
	<parameter
		name="NODE_COUNT"
		displayName="000 - Number of nodes (NODE_COUNT)"
		type="int"
		defaultValue="120"
		isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$IntConverter" />
	<parameter
		name="INITIAL_VIEW_PERC"
		displayName="001 - Percentage of values to fill for the initial view (INITIAL_VIEW_PERC)"
		type="double"
		defaultValue="0.59"
		isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter" />
	<parameter
		name="MEAN_LATENCY"
		displayName="002 - Mean latency of the system (gaussian) (MEAN_LATENCY)"
		type="double"
		defaultValue="0.013"
		isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter" />
	<parameter
		name="VAR_LATENCY"
		displayName="003 - Variance latency of system (gaussian) (VAR_LATENCY)"
		type="double"
		defaultValue="0.003"
		isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter" />
	<parameter
		name="EVENTS_RATE"
		displayName="004 - Rate of events inserted in the system (EVENTS_RATE)"
		type="double"
		defaultValue="45.0"
		isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter" />
	<parameter
		name="EVENTS_VAR_RATE"
		displayName="005 - Variance of EVENTS_RATE (EVENTS_VAR_RATE)"
		type="double"
		defaultValue="5.0"
		isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter" />
	<parameter
		name="DROPPED_RATE"
		displayName="006 - Probability of losing a message(DROPPED_RATE)"
		type="double"
		defaultValue="0.1"
		isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter" />
	<parameter
		name="DEATH_RATE"
		displayName="007 - Percentage of nodes to kill (DEATH_RATE)"
		type="double"
		defaultValue="0.05"
		isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter" />
	<parameter
		name="EXPECTED_STABLE_TIME"
		displayName="008 - Expected time to fulfill percentage of nodes killed (EXPECTED_STABLE_TIME)"
		type="double"
		defaultValue="690.0"
		isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter" />
	<parameter
		name="DRIFT_PER_SECOND"
		displayName="009 - Clock drift for each second (DRIFT_PER_SECOND)"
		type="double"
		defaultValue="1.0E-5"
		isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter" />

	<!-- Node options -->	
	<parameter
		name="fake2"
		displayName="1**********************************************"
		type="java.lang.String"
		defaultValue="************* NODE OPTIONS *************"
		isReadOnly="true"
		converter="repast.simphony.parameter.StringConverterFactory$StringStringConverter"/>
	
	<parameter
		name="GOSSIP_INTERVAL"
		displayName="100 - Gossip interval (GOSSIP_INTERVAL)"
		type="double"
		defaultValue="30.0"
		isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter" />
	<parameter
		name="EVENTS_SIZE"
		displayName="101 - Event buffer size(EVENTS_SIZE)"
		type="int"
		defaultValue="60"
		isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$IntConverter" />
	<parameter
		name="EVENT_IDS_SIZE"
		displayName="102 - Event ids buffer size(EVENT_IDS_SIZE)"
		type="int"
		defaultValue="120"
		isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$IntConverter" />
	<parameter
		name="SUBS_SIZE"
		displayName="103 - Sub buffer size (SUBS_SIZE)"
		type="int"
		defaultValue="5"
		isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$IntConverter" />
	<parameter
		name="UN_SUBS_SIZE"
		displayName="104 - Unsub buffer size(UN_SUBS_SIZE)"
		type="int"
		defaultValue="5"
		isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$IntConverter" />
	<parameter
		name="VIEWS_SIZE"
		displayName="105 - View size (VIEWS_SIZE)"
		type="int"
		defaultValue="5"
		isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$IntConverter" />
	<parameter
		name="FANOUT_SIZE"
		displayName="106 - Fanout size (FANOUT_SIZE)"
		type="int"
		defaultValue="3"
		isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$IntConverter" />
	<parameter
		name="REQUEST_TIMEOUT_ROUNDS"
		displayName="107 - Timeout after which to try another retrieve (REQUEST_TIMEOUT_ROUNDS)"
		type="int"
		defaultValue="1"
		isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$IntConverter" />
	<parameter
		name="OLD_TIME_RETRIEVE"
		displayName="108 - Number of rounds before trying to retrieve (OLD_TIME_RETRIEVE)"
		type="int"
		defaultValue="1"
		isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$IntConverter" />

	<parameter
		name="SUBS_OPTIMIZATION"
		displayName="109 - Optimize subs/view using frequency (SUBS_OPTIMIZATION)"
		type="boolean"
		defaultValue="false"
		isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$BooleanConverter" />
	<parameter
		name="K"
		displayName="110 - Threshold for deletion based on avg frequency (K)"
		type="double"
		defaultValue="1.0"
		isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter" />

	<parameter
		name="EVENTS_OPTIMIZATION_FIRST"
		displayName="111 - First filter to use per events(EVENTS_OPTIMIZATION_FIRST)"
		type="java.lang.String"
		defaultValue="NONE"
		isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$StringStringConverter"
		values="NONE UNIFORM SIZE " />
	<parameter
		name="EVENTS_OPTIMIZATION_SECOND"
		displayName="112 - Second/final filter to use per events(EVENTS_OPTIMIZATION_SECOND)"
		type="java.lang.String"
		defaultValue="RANDOM"
		isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$StringStringConverter"
		values="RANDOM TIMESTAMP AGE " />
	<parameter
		name="EVENT_IDS_OPTIMIZATION_FIRST"
		displayName="113 - First filter to use per eventIds(EVENT_IDS_OPTIMIZATION_FIRST)"
		type="java.lang.String"
		defaultValue="NONE"
		isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$StringStringConverter"
		values="NONE UNIFORM SIZE " />
	<parameter
		name="EVENT_IDS_OPTIMIZATION_SECOND"
		displayName="114 - Second/final filter to use per eventIds(EVENT_IDS_OPTIMIZATION_SECOND)"
		type="java.lang.String"
		defaultValue="RANDOM"
		isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$StringStringConverter"
		values="RANDOM TIMESTAMP " />
	<parameter
		name="LONG_AGO"
		displayName="115 - Number of events after which an older event is deleted (LONG_AGO)"
		type="long"
		defaultValue="3"
		isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$LongConverter" />
		
	<parameter
		name="RETRIEVE_METHOD"
		displayName="115 - Retrieve method(RETRIEVE_METHOD)"
		type="java.lang.String"
		defaultValue="PAPER"
		isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$StringStringConverter"
		values="MINE PAPER " />
	
	<!-- Roba di repast -->
	<parameter
		name="fake2"
		displayName="2**********************************************"
		type="java.lang.String"
		defaultValue="************* NODE OPTIONS *************"
		isReadOnly="true"
		converter="repast.simphony.parameter.StringConverterFactory$StringStringConverter"/>
	
	<parameter
		name="randomSeed"
		displayName="Default Random Seed"
		type="int"
		defaultValue="__NULL__"
		isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$IntConverter" />
	<parameter
		name="messagedirected"
		displayName="message directed"
		type="java.lang.Boolean"
		defaultValue="true"
		isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$BooleanConverter" />
	<parameter
		name="viewdirected"
		displayName="view directed"
		type="java.lang.Boolean"
		defaultValue="true"
		isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$BooleanConverter" />
</parameters>